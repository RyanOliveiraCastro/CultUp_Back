<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="poo1" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
    <properties>
      <property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQLDialect"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
<!--      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://cultup.cg2onikyzsv6.sa-east-1.rds.amazonaws.com/cultup"/>
      <property name="javax.persistence.jdbc.user" value="ryanoliveira"/>
      <property name="javax.persistence.jdbc.password" value="ryan123456"/>-->
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://127.0.0.1:5433/cultup"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.jdbc.password" value="123456"/>
      <property name="hibernate.show_sql" value="true"/>
      <property name="hibernate.format_sql" value="true"/>
      <property name="hibernate.use_sql_comments" value="false"/>
      <property name="hibernate.jdbc.wrap_result_sets" value="false"/>
      <property name="hibernate.hibernate.cache.use_query_cache" value="true"/>      
      <property name="hibernate.hbm2ddl.auto" value="none"/>
      <property name="hibernate.connection.autocommit" value="true" />
    </properties>
  </persistence-unit>
</persistence>